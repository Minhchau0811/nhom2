@page "/processedorders"
@attribute [Authorize]
@inject IViewProcessedOrderUseCase viewProcessedOrderUseCase

<h3>Đơn hàng đã được xử lý</h3>

<br />
<OrderListComponent Orders="orders"></OrderListComponent>
@code {
    private IEnumerable<Order> orders;
    protected override void OnInitialized()
    {
        base.OnInitialized();
        orders = viewProcessedOrderUseCase.Execute();
    }

}
